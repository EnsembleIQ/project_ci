{
    "name": "ensembleiq/project_ci",
    "description": "Fork of fourkitchens/project_ci. Scripts to help aid in deployment for sites.",
    "authors": [
        {
            "name": "Allan Chappell",
            "email": "allan@fourkitchens.com"
        }
    ],
    "require": {
        "drupal/core-composer-scaffold": "*"
    },
    "extra": {
        "drupal-scaffold": {
            "file-mapping": {
                "[project-root]/.circleci/config.yml": {
                    "mode": "replace",
                    "path": "config.yml",
                    "overwrite": false
                }
            }
        }
    },
    "conflict": {
        "fourkitchens/project_ci": "*",
        "fourkitchens/pots": "*"
    }
}
